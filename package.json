{
  "name": "n8n-nodes-instasent",
  "version": "0.2.0",
  "description": "n8n node for Instasent integration",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "instasent",
    "sms"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Instasent",
    "email": "dev@instasent.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-instasent/n8n-nodes-instasent.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/InstasentIngestApi.credentials.js",
      "dist/credentials/InstasentProductApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Instasent/Ingest/InstasentIngest.node.js",
      "dist/nodes/Instasent/Product/InstasentProduct.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-core": "*",
    "n8n-workflow": "*"
  }
}
